{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "car-interface-FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backend.interface.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--reload"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "car-interface-worker",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.interface.celery_config.celery_app",
                "worker",
                "--loglevel=info"
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "car-interface-flower",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.interface.celery_config.celery_app",
                "flower",
                "--port=5555"
            ],
            "console": "integratedTerminal",
        }
    ],
    "compounds": [
        {
            "name": "car-interface",
            "configurations": ["car-interface-FastAPI","car-interface-worker","car-interface-flower"],
            "stopAll": true,
            "presentation": {
                "hidden": false,
                "group": "",
                "order": 1
            }
        }
    ]
}